module sc_computer_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clock;
reg mem_clk;
reg resetn;
// wires                                               
wire [31:0] aluout;
wire dmem_clk;
wire imem_clk;
wire [31:0] inst;
wire [31:0] memout;
wire [31:0] pc;

wire sampler;                             

// assign statements (if any)                          
sc_computer i1 (
// port map - connection between master ports and signals/registers   
	.aluout(aluout),
	.clock(clock),
	.dmem_clk(dmem_clk),
	.imem_clk(imem_clk),
	.inst(inst),
	.mem_clk(mem_clk),
	.memout(memout),
	.pc(pc),
	.resetn(resetn)
);

// resetn
initial
begin
	resetn = 1'b0;
	resetn = #40000 1'b1;
end 

// clock
always
begin
	clock = 1'b0;
	clock = #10000 1'b1;
	#10000;
end 

// mem_clk
initial
begin
	mem_clk = 1'b1;
	# 5000;
	repeat(199)
	begin
		mem_clk = 1'b0;
		mem_clk = #5000 1'b1;
		# 5000;
	end
	mem_clk = 1'b0;
end 

sc_computer_vlg_sample_tst tb_sample (
	.clock(clock),
	.mem_clk(mem_clk),
	.resetn(resetn),
	.sampler_tx(sampler)
);

sc_computer_vlg_check_tst tb_out(
	.aluout(aluout),
	.dmem_clk(dmem_clk),
	.imem_clk(imem_clk),
	.inst(inst),
	.memout(memout),
	.pc(pc),
	.sampler_rx(sampler)
);
endmodule

